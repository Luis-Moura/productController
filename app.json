{
  "expo": {
    "name": "productController",
    "slug": "productController",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "productcontroller",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.luisdev1.productController"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "56092d23-36a6-49cd-8535-7a47af3dc8c5"
      }
    }
  }
}
